{
  "name": "@geoapify/route-planner-sdk",
  "version": "0.1.0",
  "description": "A TypeScript SDK for Geoapify Route Planner API",
  "main": "dist/route-planner-sdk.cjs.js",
  "typings": "dist/route-planner-sdk.d.ts",
  "minimized": "dist/route-planner-sdk.umd.js",
  "module": "dist/route-planner-sdk.esm.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "build-all": "npm run build && npm run build-minimized",
    "build-minimized": "rollup -c rollup-config.mjs",
    "demo-vanilla": "open -a 'Google Chrome' demo/html-vanilla/demo.html",
    "demo-node-new": "node demo/node-new/demo.js",
    "demo-node-old": "node demo/node-old/demo.js"
  },
  "type": "module",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.12",
    "node-fetch": "^3.3.2",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^4.9.5",
    "rollup": "^4.21.2",
    "tslib": "^2.7.0"
  }
}
